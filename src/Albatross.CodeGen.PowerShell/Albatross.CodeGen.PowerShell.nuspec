<?xml version="1.0"?>
<package>
  <metadata>
    <id>Albatross.CodeGen.PowerShell</id>
    <version>1.1.9.5</version>
    <title>Albatross Code Generator PowerShell Utilities</title>
    <authors>Rushui Guan</authors>
